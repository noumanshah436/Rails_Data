<%= text_area_tag(:message, "Hi, nice site", size: "24x6") %> <br> <br>
<%= password_field_tag(:password) %> <br> <br>
<%= hidden_field_tag(:parent_id, "5") %> <br> <br>
<%= search_field(:user, :name) %> <br> <br>
<%= telephone_field(:user, :phone) %> <br> <br>
<%= date_field(:user, :born_on) %> <br> <br>
<%= datetime_local_field(:user, :graduation_day) %> <br> <br>
<%= month_field(:user, :birthday_month) %> <br> <br>
<%= week_field(:user, :birthday_week) %> <br> <br>
<%= url_field(:user, :homepage) %> <br> <br>
<%= email_field(:user, :address) %> <br> <br>
<%= color_field(:user, :favorite_color) %> <br> <br>
<%= time_field(:task, :started_at) %> <br> <br>
<%= number_field(:product, :price, in: 1.0..20.0, step: 0.5) %> <br> <br>
<%= range_field(:product, :discount, in: 1..100) %>


