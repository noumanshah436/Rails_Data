<% if notice %>
  <div class="alert alert-success alert-dismissible fade show mx-3 my-1" id="alert" role="alert">
    <strong><%= notice %></strong>
  </div>
<% end %>
<% if flash[:alert] %>
  <div class="alert alert-danger alert-dismissible fade show mx-3 my-1" id="alert" role="alert">
    <strong> <%= flash[:alert] %></strong>
  </div>
<% end %>
